                                       Stretch="Uniform" />
                            </Grid>
                            <Rectangle Name="divider"
                                       Width="2"
                                       Height="25"
                                       Margin="5,0,0,0"
                                       Fill="#e7c169"
                                       Opacity="0.5" />
                            <TextBlock Name="backtext"
                                       Height="30"
