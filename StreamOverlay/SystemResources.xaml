    <ImageBrush x:Key="campaignlevel_shadow"
                ImageSource="/resources/images/campaignmenu/levels/lvlcard_shadow.png"
                Stretch="Uniform" />
    <Style x:Key="CampaignListItem"
           TargetType="ListBoxItem">
        <Setter Property="Width"
                Value="560" />
        <Setter Property="Height"
                Value="130" />
        <Setter Property="Margin"
                Value="0,-5,0,0" />
        <Setter Property="Background"
                Value="{StaticResource campaignlevel_shadow}" />
        <Setter Property="FocusVisualStyle"
                Value="{x:Null}" />
        <Setter Property="RenderTransformOrigin"
                Value="0.5, 0.5" />
        <Setter Property="RenderTransform">
            <Setter.Value>
                <ScaleTransform ScaleX="1"
                                ScaleY="1" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ListBoxItem">
                    <Grid>
                        <Grid x:Name="cardgrid">
                            <Image x:Name="shadow"
                                   Width="560"
                                   Height="130"
                                   Margin="0,0,0,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   Opacity="1"
                                   Source="/resources/images/campaignmenu/levels/lvlcard_shadow.png" />
                            <Image x:Name="glow"
                                   Width="560"
                                   Height="130"
                                   Margin="0,0,0,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   Opacity="0"
                                   Source="/resources/images/campaignmenu/levels/lvlcard_glow.png" />
                            <Image x:Name="glow2"
                                   Width="560"
                                   Height="130"
                                   Margin="0,0,0,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   Opacity="0"
                                   Source="/resources/images/campaignmenu/levels/lvlcard_glow.png" />
                            <Grid Width="560"
                                  Height="130"
                                  Margin="0,0,0,0"
                                  Background="Gray"
                                  Opacity="1">
                                <Grid.OpacityMask>
                                    <ImageBrush ImageSource="/resources/images/campaignmenu/levels/lvlcard_mask.png" />
                                </Grid.OpacityMask>
                                <Image x:Name="image"
                                       Width="560"
                                       Height="130"
                                       Margin="0,0,0,0"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Source="{Binding ImageName}" />
                                <Image x:Name="titlebg"
                                       Width="560"
                                       Height="45"
                                       Margin="0,0,0,0"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Bottom"
                                       Source="/resources/images/campaignmenu/levels/titlebg.png" />
                                <Image x:Name="highlight"
                                       Width="560"
                                       Height="130"
                                       Margin="0,0,0,0"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Opacity="0"
                                       Source="/resources/images/campaignmenu/levels/lvlcard_highlight.png" />
                            </Grid>
                        </Grid>
                        <Rectangle Width="40"
                                   Height="40"
                                   Margin="0,10,5,0"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Top">
                            <Rectangle.Fill>
                                <ImageBrush ImageSource="{Binding Type}" />
                            </Rectangle.Fill>
                        </Rectangle>
                        <TextBlock Margin="0,0,0,11"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Bottom"
                                   Style="{StaticResource CampaignRegularTextBlockStyle}"
                                   Text="{Binding Name}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver"
                                 Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard Name="MouseOverStoryBoard">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1.025"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)"
                                                         To="1.025"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetName="glow"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0.5"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation AutoReverse="True"
                                                         RepeatBehavior="Forever"
                                                         Storyboard.TargetName="glow2"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0.5"
                                                         Duration="0:0:.8" />
                                        <DoubleAnimation AutoReverse="True"
                                                         RepeatBehavior="Forever"
                                                         Storyboard.TargetName="highlight"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="1"
                                                         Duration="0:0:.8" />
                                    </Storyboard>
                                </BeginStoryboard>
                                <!--<SoundPlayerAction Source="resources/audio/mouseover_01.wav" />-->
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard Name="MouseOverStoryBoard2">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)"
                                                         To="1"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetName="glow"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0"
                                                         Duration="0:0:.5" />
                                        <DoubleAnimation Storyboard.TargetName="glow2"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0"
                                                         Duration="0:0:.5" />
                                        <DoubleAnimation Storyboard.TargetName="highlight"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0"
                                                         Duration="0:0:.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsFocused"
                                 Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1.025"
