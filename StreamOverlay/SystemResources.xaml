    <Style x:Key="MainMenuButtonFocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Name="focusborder"
                          Width="auto"
                          Height="40"
                          MinWidth="100">
                        <Grid Name="buttoncontainer"
                              RenderTransformOrigin="0.5, 0.5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="2" />
                            </Grid.ColumnDefinitions>
                            <Grid.RenderTransform>
                                <ScaleTransform ScaleX="1"
                                                ScaleY="1" />
                            </Grid.RenderTransform>
                            <Rectangle Name="strokeblack"
                                       Grid.ColumnSpan="3"
                                       Height="40"
                                       Fill="Transparent"
                                       RadiusX="5"
                                       RadiusY="5"
                                       Stroke="Black"
                                       StrokeThickness="4" />
                            <Rectangle Name="strokegold"
                                       Grid.Column="1"
                                       Height="38"
                                       Fill="Transparent"
                                       RadiusX="5"
                                       RadiusY="5"
                                       Stroke="Red"
                                       StrokeThickness="2" />
                            <Rectangle Name="strokegoldover"
                                       Grid.Column="1"
                                       Height="38"
                                       Fill="Transparent"
                                       Opacity="0"
                                       RadiusX="5"
                                       RadiusY="5"
                                       Stroke="{StaticResource MainMenuButtonStrokeGradientOver}"
                                       StrokeThickness="2" />
                        </Grid>
                    </Grid>
                </ControlTemplate>
