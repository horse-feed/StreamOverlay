                    Grid.ColumnSpan="2"
                    Background="{StaticResource ComboBoxInnerGradient}"
                    BorderBrush="{StaticResource ComboBoxStrokeGradient}"
                    BorderThickness="2"
                    CornerRadius="4" />
            <Border Grid.Column="0"
                    Margin="1"
                    Background="Transparent"
                    BorderBrush="Transparent"
                    BorderThickness="0,0,1,0"
                    CornerRadius="2,0,0,2" />
            <Path x:Name="Arrow"
                  Grid.Column="1"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  Data="M 0 0 L 4 4 L 8 0 Z"
                  Fill="#b49d64"
                  Visibility="Hidden" />
            <Image Grid.Column="1"
                   Width="25"
                   Height="25"
                   Margin="-20,0,0,0"
                   Source="{StaticResource combo_arrow}" />
