    <Style TargetType="{x:Type Button}"
           x:Key="HomeCityHUDButton">
        <Setter Property="Focusable"
                Value="False" />
        <Setter Property="Height"
                Value="100" />
        <Setter Property="Width"
                Value="100" />
        <Setter Property="RenderTransformOrigin"
                Value="0.5, 0.5" />
        <Setter Property="RenderTransform">
            <Setter.Value>
                <ScaleTransform ScaleX="1"
                                ScaleY="1" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Grid x:Name="ButtonBody"
                              Height="100"
                              Width="100"
                              RenderTransformOrigin="0.5,0.5">
                            <Grid.RenderTransform>
                                <ScaleTransform />
                            </Grid.RenderTransform>
                            <Image Height="100"
                                   Width="100"
                                   Source="{DynamicResource TwitchPanelLogo}"></Image>
                            <Grid Height="100"
                                  Width="100">
                                <Grid.OpacityMask>
                                    <ImageBrush ImageSource="/resources/images/hud/hud_hcflag_mask.png" />
                                </Grid.OpacityMask>
                                <!-- Flag Image -->
                                <Image Width="80"
                                       Height="80"
                                       Source="{Binding TwitchIcon, IsAsync=True, Mode=OneWay}">
                                    <Image.OpacityMask>
                                        <ImageBrush ImageSource="/resources/images/hud/hud_hcflag_mask.png" />
                                    </Image.OpacityMask>
                                </Image>
                                <Grid Height="80"
                                      Width="80"
                                      HorizontalAlignment="Center"
                                      VerticalAlignment="Center"
                                      Background="Transparent">
                                    <Grid.OpacityMask>
                                        <ImageBrush ImageSource="/resources/images/hud/hud_hcflag_mask.png" />
                                    </Grid.OpacityMask>
                                </Grid>
                                <Ellipse Width="100"
                                         Height="100"
                                         x:Name="glow"
                                         Opacity="0">
                                    <Ellipse.Fill>
                                        <ImageBrush ImageSource="resources/images/hud/hud_hcflag_glow.png" />
                                    </Ellipse.Fill>
                                </Ellipse>
                                <Ellipse Width="100"
                                         Height="100"
                                         x:Name="flash_glow"
                                         Visibility="Hidden">
                                    <Ellipse.Fill>
                                        <ImageBrush ImageSource="resources/images/hud/hud_hcflag_glow.png" />
                                    </Ellipse.Fill>
                                </Ellipse>
                            </Grid>
                        </Grid>
                        <Grid HorizontalAlignment="Center"
                              VerticalAlignment="Center">
                            <TextBlock Margin="-4,0,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin=" 4,0,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin="0, 4,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin="0,-4,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin="-2.9,-2.9,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin="-2.9, 2.9,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin=" 2.9,-2.9,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Margin=" 2.9, 2.9,0,0"
                                       Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Trajan Pro 3"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="Black" />
                            <TextBlock Text="{TemplateBinding Content}"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="Trajan Pro"
                                       FontWeight="Bold"
                                       FontSize="35pt"
                                       Foreground="{StaticResource OverlayTitleGradient}">
                                <TextBlock.Effect>
                                    <DropShadowEffect BlurRadius="6"
                                                      Color="Black"
                                                      Direction="-90"
                                                      ShadowDepth="4"
                                                      RenderingBias="Quality" />
                                </TextBlock.Effect></TextBlock>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed"
                                 Value="True">
                            <Trigger.EnterActions>
                                <StopStoryboard BeginStoryboardName="MouseOverStoryBoard" />
                                <StopStoryboard BeginStoryboardName="MouseOverStoryBoard2" />
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1"
                                                         Duration="0:0:.05" />
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)"
                                                         To="1"
                                                         Duration="0:0:.05" />
                                        <DoubleAnimation Storyboard.TargetName="glow"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="1"
                                                         Duration="0:0:.05" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1.025"
                                                         Duration="0:0:.05" />
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)"
                                                         To="1.025"
                                                         Duration="0:0:.05" />
                                        <DoubleAnimation Storyboard.TargetName="glow"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0.75"
                                                         Duration="0:0:.05" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsMouseOver"
                                 Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard Name="MouseOverStoryBoard">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1.025"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)"
                                                         To="1.025"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetName="glow"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0.75"
                                                         Duration="0:0:.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard Name="MouseOverStoryBoard2">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleX)"
                                                         To="1"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetName="ButtonBody"
                                                         Storyboard.TargetProperty="RenderTransform.(ScaleTransform.ScaleY)"
                                                         To="1"
                                                         Duration="0:0:.2" />
                                        <DoubleAnimation Storyboard.TargetName="glow"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0"
                                                         Duration="0:0:.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
